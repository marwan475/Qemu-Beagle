#!/bin/sh
#
# see: dh_installdeb(1)

set -e
if [ -f /lib/firmware/brcm/brcmfmac43455-sdio.txt ] ; then
	rm -f /lib/firmware/brcm/brcmfmac43455-sdio.txt || true
	cp -v /lib/firmware/brcm/brcmfmac43455-sdio.beagleboard.org,am5729-beagleboneai.txt /lib/firmware/brcm/brcmfmac43455-sdio.txt
else
	cp -v /lib/firmware/brcm/brcmfmac43455-sdio.beagleboard.org,am5729-beagleboneai.txt /lib/firmware/brcm/brcmfmac43455-sdio.txt
fi

if [ -f /lib/firmware/brcm/brcmfmac43455.bin ] ; then
	rm -f /lib/firmware/brcm/brcmfmac43455.bin || true
	cp -v /lib/firmware/brcm/brcmfmac43455-sdio_am5729-beagleboneai.bin /lib/firmware/brcm/brcmfmac43455.bin
else
	cp -v /lib/firmware/brcm/brcmfmac43455-sdio_am5729-beagleboneai.bin /lib/firmware/brcm/brcmfmac43455.bin
fi

if [ ! -d /lib/firmware/cypress/ ] ; then
	mkdir -p /lib/firmware/cypress/
fi

if [ -f /lib/firmware/cypress/cyfmac43455-sdio.txt ] ; then
	rm -f /lib/firmware/cypress/cyfmac43455-sdio.txt || true
	cp -v /lib/firmware/brcm/brcmfmac43455-sdio.beagleboard.org,am5729-beagleboneai.txt /lib/firmware/cypress/cyfmac43455-sdio.txt
else
	cp -v /lib/firmware/brcm/brcmfmac43455-sdio.beagleboard.org,am5729-beagleboneai.txt /lib/firmware/cypress/cyfmac43455-sdio.txt
fi

if [ -f /lib/firmware/cypress/cyfmac43455-sdio.bin ] ; then
	rm -f /lib/firmware/cypress/cyfmac43455-sdio.bin || true
	cp -v /lib/firmware/bbb.io/cyfmac43455-sdio.bin /lib/firmware/cypress/
else
	cp -v /lib/firmware/bbb.io/cyfmac43455-sdio.bin /lib/firmware/cypress/
fi

if [ -f /lib/firmware/cypress/cyfmac43455-sdio.clm_blob ] ; then
	rm -f /lib/firmware/cypress/cyfmac43455-sdio.clm_blob || true
	cp -v /lib/firmware/bbb.io/cyfmac43455-sdio.clm_blob /lib/firmware/cypress/
else
	cp -v /lib/firmware/bbb.io/cyfmac43455-sdio.clm_blob /lib/firmware/cypress/
fi

# dh_installdeb will replace this with shell code automatically
# generated by other debhelper scripts.



exit 0

